    "@supabase/supabase-js": "^2.81.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@whiskeysockets/baileys": "^7.0.0-rc.6",
    "autoprefixer": "^10.4.22",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
